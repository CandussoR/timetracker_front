{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/timetracker-backend",
          "sidecar": true
        }
      ]
    },
    "dialog:default",
    "os:default"
  ]
}